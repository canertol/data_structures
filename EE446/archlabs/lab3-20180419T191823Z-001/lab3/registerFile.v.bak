module RegisterFile
	#(parameter W = 32)
	( input wire clk, WE3, 
	  input wire [W-1:0] RA1, RA2, WA3,
	  input wire [W-1:0] WD3, R15,
	  output wire [W-1:0] RD1, RD2);
	
	reg [W-1:0] rf[14:0];
	
	assign RD1 = (RA1 == 4'b1111) ? R15 : rf[RA1];
	assign RD2 = (RA2 == 4'b1111) ? R15 : rf[RA2];
	
	always @(posedge clk)
	begin
		if(WE3 == 1'b1)
			rf[WA3] <= WD3;
	
	end
endmodule 